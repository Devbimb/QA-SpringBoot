package com.example.model;

import javax.persistence.Entity;

@Entity
public class Product {
	public int id;
	public String name;
	public String brand;
	public Product(int id, String name, String brand) {
		super();
		this.id = id;
		this.name = name;
		this.brand = brand;
	}
	public Product() {
		super();
	}
	
	

}
